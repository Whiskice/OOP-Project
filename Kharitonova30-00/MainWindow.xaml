<Window x:Class="Kharitonova30_00.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kharitonova30_00"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Frame x:Name="FrameMain" Content="Frame" Height="300" Margin="0,22,0,112" NavigationUIVisibility="Visible"/>
        <Button x:Name="AddButton" Content="Добавить" HorizontalAlignment="Left" Margin="70,0,0,63" Width="100" FontFamily="GOST type B" FontSize="14" FontWeight="Bold" Height="30" VerticalAlignment="Bottom" Click="AddButton_Click"/>
        <Button x:Name="DeleteButton" Content="Удалить" Margin="0,0,69,63" FontFamily="GOST type B" FontSize="14" FontWeight="Bold" Height="30" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="100"/>
    </Grid>
</Window>
